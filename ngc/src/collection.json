{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "fsClass": {
      "description": "A firebase firestore class schematic.",
      "factory": "./firestore-class/index#fsClass"
    },
    "ngcComponent": {
      "description": "A default component.",
      "factory": "./ngc-component/index#ngcComponent",
      "schema": "./_schemas/name-schema.json"

    },
    "ngcModule": {
      "description": "A default module.",
      "factory": "./ngc-module/index#ngcModule",
      "schema": "./_schemas/name-schema.json"
    },
    "ngcClass": {
      "description": "A default class.",
      "factory": "./ngc-class/index#ngcClass",
      "schema": "./_schemas/name-schema.json"
    },
    "ngcService": {
      "description": "A Default service.",
      "factory": "./ngc-service/index#ngcService",
      "schema": "./_schemas/name-schema.json"
    },
    "ngcServiceFirestore": {
      "description": "A Firestore CRUD service.",
      "factory": "./ngc-service-firestore/index#ngcServiceFirestore",
      "schema": "./_schemas/name-prompt-schema.json"
    },
    "ngc-routing": {
      "description": "A blank schematic.",
      "factory": "./ngc-routing/index#ngcRouting"
    }
  }
}
